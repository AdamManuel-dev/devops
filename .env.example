# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/intelliops
REDIS_URL=redis://localhost:6379

# Kafka Configuration
KAFKA_BROKERS=localhost:9092,localhost:9093,localhost:9094
KAFKA_CLIENT_ID=intelliops-ai-agent
KAFKA_GROUP_ID=intelliops-consumers

# Vector Database
PINECONE_API_KEY=your_pinecone_api_key
PINECONE_ENVIRONMENT=your_pinecone_environment
PINECONE_INDEX_NAME=intelliops-embeddings

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=text-embedding-ada-002

# Coralogix Integration
CORALOGIX_API_KEY=your_coralogix_api_key
CORALOGIX_ENDPOINT=https://api.coralogix.com
CORALOGIX_TEAM_ID=your_team_id
CORALOGIX_APPLICATION_NAME=intelliops

# Sentry Integration
SENTRY_DSN=your_sentry_dsn
SENTRY_ORG=your_sentry_org
SENTRY_PROJECT_SLUGS=project1,project2

# Prometheus Configuration
PROMETHEUS_URL=http://localhost:9090
PROMETHEUS_TOKEN=your_prometheus_token

# GitHub Integration
GITHUB_TOKEN=your_github_token
GITHUB_ORG=your_organization

# Slack/Teams Integration
SLACK_BOT_TOKEN=your_slack_bot_token
SLACK_SIGNING_SECRET=your_slack_signing_secret
TEAMS_BOT_ID=your_teams_bot_id
TEAMS_BOT_PASSWORD=your_teams_password

# JIRA Integration
JIRA_URL=https://your-domain.atlassian.net
JIRA_EMAIL=your_email@domain.com
JIRA_API_TOKEN=your_jira_api_token

# Notion Integration
NOTION_API_KEY=your_notion_api_key
NOTION_DATABASE_ID=your_notion_database_id

# Security
JWT_SECRET=your_jwt_secret_key
API_KEY=your_api_key
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001,https://your-domain.com

# Application Configuration
NODE_ENV=development
PORT=3000
LOG_LEVEL=info

# Kubernetes Configuration
KUBECONFIG=/path/to/kubeconfig
NAMESPACE=intelliops